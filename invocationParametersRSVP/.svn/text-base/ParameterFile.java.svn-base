package invocationParametersRSVP;

import RSVP.ReadOnlyTextFile;

import jtextpro.JTextPro;

/**
 * 
 * @author mb
 * 
 */
public class ParameterFile extends ReadOnlyTextFile {

	public ParameterFile(String fullFileName) {
		super(fullFileName);
	}

	public static boolean isCommentLine(String s) {
		return s.charAt(0) == '#';
	}

}
